BEGIN;
    CREATE: EX(1, 1): MARK(1);

TECNI QUEUE,  1;
    SEIZE:  TECNICO;
    DELAY:  EX(2, 2);
    RELEASE: TECNICO;

ESTA QUEUE, 2;
    SEIZE:  INSPECTOR;
    DELAY:  UN(3, 3);
    RELEASE: INSPECTOR; 
    BRANCH, 1:
        WITH, 0.75, AJUS:
        WITH, 0.25, EMPA; 

AJUS QUEUE, 3;
    SEIZE:  TECNICOAJUS;
    DELAY:  RN(4, 4);
    RELEASE: TECNICOAJUS:NEXT(ESTA);

EMPA QUEUE, 4;
    SEIZE:  EMPAQUE;
    RELEASE: EMPAQUE:DISPOSE;
END;