BEGIN;
    CREATE: EX(1, 1): MARK(1);
    ASSIGN: A(2)=2:A(3)=0: NEXT(MAQUI1);

    CREATE: EX(2, 2): MARK(2);
    ASSIGN: A(2)=1:A(3)=0: NEXT(MAQUI2);

MAQUI1 QUEUE,1;
    SEIZE: MAQUINA1;
    DELAY: EX(3, 3);S
    RELEASE: MAQUINA1:NEXT(MONTA);

MAQUI2 QUEUE,2; 
    SEIZE: MAQUINA2;
    DELAY: EX(4, 4);
    RELEASE: MAQUINA2:NEXT(MONTA);

MONTA QUEUE,3;
    SEIZE: MONTACARGA;
    DELAY: EX(5, 5);
    RELEASE: MONTACARGA;

BRANCH, 1:
    IF, A(2).EQ.1, MAQUI2:
    IF, A(2).EQ.2, MAQUI1:
    IF, A(2).EQ.3, MAQUI3:
    IF, A(2).EQ.4, MAQUI4:
    ELSE, MAQUI4;

MAQUI3 QUEUE,4; 
    SEIZE: MAQUINA3;
    DELAY: EX(6, 6);
    RELEASE: MAQUINA3;

INSPE QUEUE,5;
    SEIZE: INSPECTOR;
    DELAY: UN(7, 7);
    RELEASE: INSPECTOR;
    BRANCH, 1:
        WITH, 0.60, EMPAQUE:
        WITH, 0.40, MONTA;

EMPAQUE QUEUE,6;
    SEIZE: EMPAQUETADOR;
    DELAY: RN(8, 8);
    RELEASE: EMPAQUETADOR:DISPOSE;

MAQUI4 QUEUE,7;
    SEIZE: MAQUINA4;
    DELAY: EX(9, 9);
    RELEASE: MAQUINA4:NEXT(MAQUI3);

END;